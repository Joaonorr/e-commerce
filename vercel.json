{ 
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "buildCommand": "next build",
    "routes": [
        { 
            "src": "/.*",
            "dest": "src/main.ts",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        }
    ],
    "projectSettings": {
        "devCommand": null,
        "framework": "nextjs",
        "commandForIgnoringBuildStep": "",
        "installCommand": null,
        "outputDirectory": null
    }
}